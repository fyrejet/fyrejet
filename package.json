    "cookie-signature": "^1.1.0",
    "debug": "^4.2.0",
    "depd": "^2.0.0",
    "encodeurl": "~1.0.2",
    "escape-html": "~1.0.3",
    "etag": "~1.8.1",
    "express": "^4.17.1",
    "fast-decode-uri-component": "^1.0.1",
    "finalhandler": "~1.1.2",
    "fresh": "0.5.2",
    "lru-cache": "^6.0.0",
    "methods": "~1.1.2",
    "mnemonist": "^0.38.3",
    "nanoid": "^4.0.0",
    "on-change": "^3.0.2",
    "on-finished": "~2.3.0",
    "parseurl": "~1.3.3",
    "path-to-regexp": "0.1.7",
    "proxy-addr": "~2.0.5",
    "qs": "^6.10.5",
    "range-parser": "~1.2.1",
    "safe-buffer": "^5.2.1",
    "send": "0.17.1",
    "serve-static": "1.14.1",
    "setprototypeof": "^1.2.0",
    "statuses": "^2.0.1",
